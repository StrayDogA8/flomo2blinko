{
  "manifest_version": 3,
  "name": "flomo2blinko",
  "version": "1.0",
  "description": "Convert Flomo notes to Blinko format",
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "icons": {
    "128": "images/icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "images/icon128.png"
  },
  "permissions": [
    "downloads"
  ]
} 